@import './vars.styl';

html, body
  font-family 'Avenir', Helvetica, Arial, sans-serif
  font-weight normal
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  color mainBlack
  font-size 14px

ul
  list-style none
  margin 0
  padding 0

input
  outline none
  border 0

h3
  padding 0
  margin 0

a
  text-decoration none

{
  "name": "react-redux-app-cli",
  "version": "1.0.0",
  "description": "A CLI for scaffolding React.js projects.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --progress --colors",
    "build": "rm -rf ./build && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --colors",
    "rm:build": "rm -rf ./build",
    "lint": "eslint ./app",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "mock": "node --harmony ./mock/server.js"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Ann Huang",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "koa": "^2.2.0",
    "koa-body": "^2.0.1",
    "koa-router": "^7.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss": "^6.0.1",
    "postcss-loader": "^2.0.5",
    "pre-commit": "^1.2.2",
    "style-loader": "^0.18.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "es6-promise": "^4.1.0",
    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-addons-perf": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.3"
  }
}
